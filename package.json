{
  "name": "graphql-academia-online",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon src/server.ts --exec ts-node src/server.ts -e ts,graphql",
    "tslint": "tslint --project tsconfig.json",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:write"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanoscz/graphql-academia-online.git"
  },
  "keywords": [
    "graphql"
  ],
  "author": "fernando castillo torrico <fernandocto.scz@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nanoscz/graphql-academia-online/issues"
  },
  "homepage": "https://github.com/nanoscz/graphql-academia-online#readme",
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "apollo-server-express": "^2.11.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.12",
    "graphql-tools": "^4.0.7",
    "http": "0.0.0",
    "lodash": "^4.17.21",
    "ncp": "^2.0.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/graphql": "^14.5.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
